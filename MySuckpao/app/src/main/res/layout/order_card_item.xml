<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="411dp"
        android:layout_height="wrap_content"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/cake_card_num"
            android:layout_width="241dp"
            android:layout_height="81dp"
            android:layout_marginStart="69dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="31dp"
            android:text="TextView"
            android:textColor="@color/green1but"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/cake_card_laundry"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cake_card_type" />

        <TextView
            android:id="@+id/cake_card_laundry"
            android:layout_width="241dp"
            android:layout_height="81dp"
            android:layout_marginStart="69dp"
            android:layout_marginTop="23dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="24dp"
            android:text="TextView"
            android:textColor="@color/green1but"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/cake_card_store"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cake_card_num" />

        <TextView
            android:id="@+id/cake_card_store"
            android:layout_width="241dp"
            android:layout_height="81dp"
            android:layout_marginStart="69dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="197dp"
            android:lineSpacingExtra="24sp"
            android:text="TextView"
            android:textColor="@color/green1but"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cake_card_laundry" />

        <TextView
            android:id="@+id/cake_card_type"
            android:layout_width="241dp"
            android:layout_height="81dp"
            android:layout_marginStart="69dp"
            android:layout_marginTop="117dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="15dp"
            android:text="TextView"
            android:textColor="@color/green1but"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/cake_card_num"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>