<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/numc"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/showNum" />

    <TextView
        android:id="@+id/laundryc"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/showNum"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/showNum"
        android:layout_width="20dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="@color/blue"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/numc"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/showLaundry"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="@color/blue"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/laundryc"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/showName"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="@color/blue"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/storec"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/showStatus"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:textColor="@color/green"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/statusc"
        app:layout_constraintTop_toBottomOf="@+id/numc" />

    <TextView
        android:id="@+id/storec"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/showLaundry"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/statusc"
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numc" />

    <TextView
        android:id="@+id/detailc"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/showStatus"
        app:layout_constraintTop_toBottomOf="@+id/laundryc" />

    <TextView
        android:id="@+id/showDetail"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:textColor="@color/blue"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/detailc"
        app:layout_constraintTop_toBottomOf="@+id/showLaundry" />

    <TextView
        android:id="@+id/pricec"
        android:layout_width="43dp"
        android:layout_height="19dp"
        android:layout_marginStart="18dp"
        android:text="TextView"
        android:textColor="@color/colorTab"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusc" />

    <TextView
        android:id="@+id/showPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/blue"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/pricec"
        app:layout_constraintTop_toBottomOf="@+id/showStatus" />

</androidx.constraintlayout.widget.ConstraintLayout>